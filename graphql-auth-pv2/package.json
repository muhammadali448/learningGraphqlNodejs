{
  "name": "ultimate-authentication-pv2",
  "license": "MIT",
  "scripts": {
    "start": "node dist/server",
    "clean": "rm -rf dist",
    "build": "npm -s run clean && npm -s run generate && tsc",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only src/schema",
    "dev": "env-cmd -f ./prisma/.env ts-node-dev --no-notify --respawn --transpileOnly src/server"
  },
  "dependencies": {
    "@nexus/schema": "0.15.0",
    "bcrypt": "^5.0.0",
    "bcryptjs": "2.4.3",
    "google-auth-library": "^6.0.6",
    "graphql-middleware": "4.0.1",
    "graphql-shield": "7.3.5",
    "graphql-yoga": "1.18.3",
    "jsonwebtoken": "8.5.1",
    "nexus-plugin-prisma": "0.18.1",
    "nodemailer": "^6.4.11"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/bcryptjs": "2.4.2",
    "@types/jest": "^26.0.13",
    "@types/jsonwebtoken": "8.5.0",
    "@types/node": "12.12.55",
    "@types/node-fetch": "^2.5.7",
    "@types/ws": "7.2.6",
    "env-cmd": "^10.1.0",
    "jest": "^26.4.2",
    "ts-jest": "^26.3.0",
    "ts-node": "9.0.0",
    "ts-node-dev": "1.0.0-pre.50",
    "typescript": "4.0.2"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
